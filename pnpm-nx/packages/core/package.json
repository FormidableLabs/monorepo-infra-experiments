{
  "name": "@formidable/monorepo-tools-pnpm-nx-dogs-core",
  "version": "0.0.1",
  "description": "Core library",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/monorepo-infra-experiments.git"
  },
  "author": "Ryan Roemer <ryan.roemer@formidable.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/monorepo-infra-experiments/issues"
  },
  "homepage": "https://github.com/FormidableLabs/monorepo-infra-experiments#readme",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "BABEL_ENV=es babel src --out-dir es --config-file ../../.babelrc.js --copy-files --extensions .tsx,.ts,.jsx,.js",
    "build:cjs": "BABEL_ENV=lib babel src --out-dir lib --config-file ../../.babelrc.js --copy-files --extensions .tsx,.ts,.jsx,.js"
  }
}
